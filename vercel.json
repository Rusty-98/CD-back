{
    "version": 2,
    "name": "server",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "headers": [
      {
        "source": "/socket.io/(.*)",
        "headers": [
          {
            "key": "Connection",
            "value": "upgrade"
          },
          {
            "key": "Upgrade",
            "value": "websocket"
          }
        ]
      }
    ]
  }
  